        {
          "default": "7.2.4",
          "label": "Redis tag",
          "name": "REDIS_TAG"
        },
        {
          "default": "America/Santo_Domingo",
          "label": "Timezone",
          "name": "TIMEZONE"
        },
        {
          "default": "true",
          "label": "Enable Logfiles",
          "name": "ENABLE_LOGS"
        },
        {
          "default": "notice",
          "label": "Log level",
          "name": "LOG_LEVEL"
        },
        {
          "label": "Redis Password",
          "name": "REDIS_PASS"
        },
        {
          "default": "v2",
          "label": "Insight tag",
          "name": "INSIGHT_TAG"
        },
        {
          "default": "5540",
          "label": "Insight port",
          "name": "INSIGHT_PORT"
        },
        {
          "default": "info",
          "label": "RI log level",
          "name": "RI_LOG_LEVEL"
        },
        {
          "default": "true",
          "label": "RI files logger",
          "name": "RI_FILES_LOGGER"
        },
        {
          "default": "true",
          "label": "RI stdout logger",
          "name": "RI_STDOUT_LOGGER"
        },
        {
          "default": "insight.example.com",
          "label": "Insight domain",
          "name": "INSIGHT_DOMAIN"
        },
        {
          "default": "proxy",
          "label": "Network name",
          "name": "NETWORK"
        }
      ]
    },
