{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 3,
      "title": "Odoo",
      "description": "Open-source business apps.",
      "categories": ["project-management"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/gruposolutema/portainer-templates/main/stacks/odoo/logo/logo.png",
      "repository": {
        "url": "https://github.com/gruposolutema/portainer-templates",
        "stackfile": "stacks/odoo/docker-compose.yml"
      },
      "env": [
        {
          "default": "code.gruposolutema.com/gruposolutema/postgres",
          "label": "PostgreSQL Image",
          "name": "POSTGRES_IMAGE"
        },
        {
          "default": "16.6",
          "label": "PostgreSQL Tag",
          "name": "POSTGRES_TAG"
        },
        {
          "default": "odoo",
          "label": "Database Username",
          "name": "DB_USER"
        },
        {
          "default": "odoo",
          "label": "Database Password for User",
          "name": "DB_PASS"
        },
        {
          "default": "postgres",
          "label": "Database Name",
          "name": "DB_NAME"
        },
        {
          "label": "Super User",
          "name": "SUPERUSER_USER"
        },
        {
          "label": "Root Password",
          "name": "SUPERUSER_PASS"
        },
        {
          "default": "unaccent", 
          "label": "Database Extension",
          "name": "DB_EXTENSION"
        },        
        {
          "default": "code.gruposolutema.com/gruposolutema/odoo",
          "label": "Odoo Image",
          "name": "ODOO_IMAGE"
        },
        {
          "default": "16.0-20241209",
          "label": "Odoo Tag",
          "name": "ODOO_TAG"
        },
          {
          "default": "odoo.example.com",
          "label": "URL",
          "name": "URL"
        },
        {
          "default": "proxy",
          "label": "Network Name",
          "name": "NETWORK"
        }
      ],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        },
        {
          "container": "/mnt/extra-addons"
        },
        {
          "container": "/mnt/backups"
        }
      ]
    },
    {
      "id": 2,
      "type": 3,
      "title": "PostgreSQL",
      "description": "PostgreSQL: The World's Most Advanced Open Source Relational Database.",
      "note": "Deploys a PostgreSQL Server.",
      "categories": ["Database"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/gruposolutema/portainer-templates/main/stacks/postgres/logo/logo.png",
      "repository": {
        "url": "https://github.com/gruposolutema/portainer-templates",
        "stackfile": "stacks/postgres/docker-compose.yml"
      },
      "env": [
        {
          "default": "code.gruposolutema.com/gruposolutema/postgres",
          "label": "PostgreSQL Image",
          "name": "POSTGRESQL_IMAGE"
        },
        {
          "default": "16.6",
          "label": "PostgreSQL Tag",
          "name": "POSTGRESQL_TAG"
        },
        {
          "default": "America/Santo_Domingo",
          "label": "Timezone",
          "name": "TIMEZONE"
        },
        {
          "label": "Root Password",
          "name": "SUPERUSER_PASS"
        },
        {
          "label": "Database Name",
          "name": "DB_NAME"
        },
        {
          "label": "Database Username",
          "name": "DB_USER"
        },
        {
          "label": "Database Password",
          "name": "DB_PASS"
        },
        {
          "default": "proxy",
          "label": "Network Name",
          "name": "NETWORK"
        }
       ],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        }
      ]
    },
    {
      "id": 3,
      "type": 3,
      "title": "Redis",
      "description": "Open-source in-memory data structure store",
      "note": "Deploys a Redis server instance as an application cache or quick-response database.",
      "categories": ["Database"],
      "platform": "linux",
      "logo": "https://raw.githubusercontent.com/gruposolutema/portainer-templates/main/stacks/redis/logo/logo.png",
      "repository": {
        "url": "https://github.com/gruposolutema/portainer-templates",
        "stackfile": "stacks/redis/docker-compose.yml"
      },
      "env": [
        {
          "default": "code.grupogruposolutema.com/gruposolutema/redis",
          "label": "Redis image",
          "name": "REDIS_IMAGE"
        },
        {
          "default": "7.4.1",
          "label": "Redis tag",
          "name": "REDIS_TAG"
        },
        {
          "default": "6379",
          "label": "Redis Port",
          "name": "REDIS_PORT"
        },
        {
          "default": "America/Santo_Domingo",
          "label": "Timezone",
          "name": "TIMEZONE"
        },
        {
          "default": "true",
          "label": "Enable Logfiles",
          "name": "ENABLE_LOGS"
        },
        {
          "default": "notice",
          "label": "Log level",
          "name": "LOG_LEVEL"
        },
        {
          "label": "Redis Password",
          "name": "REDIS_PASS"
        },
        {
          "default": "redis/redisinsight",
          "label": "Insight Image",
          "name": "INSIGHT_IMAGE"
        },
        {
          "default": "2.62.0",
          "label": "Insight Tag",
          "name": "INSIGHT_TAG"
        },
        {
          "default": "5540",
          "label": "Insight port",
          "name": "INSIGHT_PORT"
        },
        {
          "default": "info",
          "label": "RI log level",
          "name": "RI_LOG_LEVEL"
        },
        {
          "default": "true",
          "label": "RI files logger",
          "name": "RI_FILES_LOGGER"
        },
        {
          "default": "true",
          "label": "RI stdout logger",
          "name": "RI_STDOUT_LOGGER"
        },
        {
          "default": "redis.example.com",
          "label": "Insight domain",
          "name": "INSIGHT_DOMAIN"
        },
        {
          "default": "proxy",
          "label": "Network name",
          "name": "NETWORK"
        }
      ]
    }
  ]
}
